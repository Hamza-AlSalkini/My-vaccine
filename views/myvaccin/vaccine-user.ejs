<%- include("header"); %>
    <div class="sidebar-container">
        <div class="sidebar-logo">
            My vaccine
        </div>
        <ul class="sidebar-navigation">
            <li class="header">Menu</li>
            <li>
                <a href="/profile">
                    <i class="fa fa-user-circle" aria-hidden="true"></i> Dashboard
                </a>
            </li>
            <li class="header">Another Menu</li>
            <li>
                <a href="/family-members">
                    <i class="fa fa-users" aria-hidden="true"></i> Family members
                </a>
            </li>
            <li>
                <a href="/vaccine-user">
                    <i class="fa fa-heartbeat" aria-hidden="true"></i> Vaccine
                </a>
            </li>
            <li>
                <a href="/add-vaccine-user">
                    <i class="fa fa-plus" aria-hidden="true"></i> Register vaccine
                </a>
            </li>
            <li>
                <a href="/show-vaccine-log">
                    <i class="fa fa-id-card-o" aria-hidden="true"></i> Vaccinate log
                </a>
            </li>
            <li>
                <a href="/logout">
                    <i class="fa fa-sign-out" aria-hidden="true"></i> sign out
                </a>
            </li>
        </ul>
    </div>

    <div class="content-container">

        <div class="container-fluid">

            <!-- Main component for a primary marketing message or call to action -->
            <div class="jumbotron">
                <h1>Here you can find information about vaccines</h1>
            </div>
            <div class="table-responsive">
                <table class="table table-bordered" style="max-width: 100%">
                    <tr>
                        <th scope="col">Id</th>
                        <th scope="col">Vaccine</th>
                        <th scope="col">Description</th>
                        <th scope="col">Recommendation</th>
                        <th scope="col">duration(day)</th>
                    </tr>
                    <% for (const row of res1) { %>
                        <tr>
                            <td>
                                <%= row.id %>
                            </td>
                            <td>
                                <%= row.vaccine %>
                            </td>
                            <td>
                                <%= row.description %>
                            </td>
                            <td>
                                <%= row.recommendation %>
                            </td>
                            <td>
                                <%= row.duration %>
                            </td>
                        </tr>
                        <% }; %>
                </table>
            </div>
        </div>
    </div>